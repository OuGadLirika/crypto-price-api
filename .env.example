# PostgreSQL async URL example
# For local dev with Docker PostgreSQL:
DATABASE_URL=postgresql+asyncpg://qorpo_user:qorpo_password@localhost:5432/qorpo
# For local PostgreSQL:
# DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/qorpo
HOST=0.0.0.0
PORT=8000
PAGE_SIZE=10
ENABLE_UVLOOP=true
ENV=development
# Set to true only for local dev table bootstrap; in production use Alembic
RUN_CREATE_ALL=false
